---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	{% for post in site.posts limit: 10 %}
	<url>
		<loc>{{ site.url }}</loc>
	</url>
	<url>
		<loc>{{ post.url | absolute_url }}</loc>
		{% if post.updated %}
			<lastmod>{{ post.updated | date: '%Y-%m-%dT%H:%M:%S+%z' }}</lastmod>
		{% else %}
			<lastmod>{{ post.date | date: '%Y-%m-%dT%H:%M:%S+%z' }}</lastmod>
		{% endif %}
	</url>
	{% endfor %}
</urlset>
