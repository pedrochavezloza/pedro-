{
  "name": "about",
  "version": "1.0.0",
  "description": "Project for creating simple about pages.",
  "main": "index.html",
  "scripts": {
    "test": "eslint",
    "build:css": "postcss css/styles/index.css -o css/styles/index.min.css -u postcss-import postcss-cssnext cssnano -m",
